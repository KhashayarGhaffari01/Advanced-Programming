<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="603.0" prefWidth="335.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.openjfx.BlocksListController">
    <children>
        <ImageView fitHeight="603.0" fitWidth="335.0">
            <image>
                <Image url="@../../../../../photos%2520for%2520khw/Blocks.jpg" />
            </image>
        </ImageView>
        <TextField fx:id="SearchField" layoutX="70.0" layoutY="131.0" promptText="Search User">
            <cursor>
                <Cursor fx:constant="TEXT" />
            </cursor>
        </TextField>
        <TextArea fx:id="BlocksArea" editable="false" layoutX="70.0" layoutY="167.0" prefHeight="349.0" prefWidth="149.0" wrapText="true">
            <font>
                <Font name="Comic Sans MS" size="14.0" />
            </font>
            <cursor>
                <Cursor fx:constant="NONE" />
            </cursor>
        </TextArea>
        <Button fx:id="BackBtn" blendMode="OVERLAY" layoutX="25.0" layoutY="551.0" mnemonicParsing="false" onAction="#pressBackBtn" prefHeight="36.0" prefWidth="70.0">
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor></Button>
        <Button fx:id="SearchBtn" blendMode="ADD" layoutX="219.0" layoutY="131.0" mnemonicParsing="false" onAction="#pressSearchBtn" prefHeight="25.0" prefWidth="14.0">
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
        </Button>
    </children>
</AnchorPane>
