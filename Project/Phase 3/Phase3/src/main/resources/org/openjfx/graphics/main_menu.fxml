<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="603.0" prefWidth="335.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.openjfx.graphics.pages.MainMenuFXML">
   <children>
      <ImageView fitHeight="603.0" fitWidth="335.0">
         <image>
            <Image url="@../images/MainMenu.jpg" />
         </image>
      </ImageView>
      <Button fx:id="UserPersonalPageBtn" blendMode="OVERLAY" layoutX="109.0" layoutY="142.0" mnemonicParsing="false" onAction="#userPersonalPage" prefHeight="34.0" prefWidth="116.0">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Button fx:id="TimelineBtn" blendMode="OVERLAY" layoutX="110.0" layoutY="194.0" mnemonicParsing="false" onAction="#timeline" prefHeight="34.0" prefWidth="116.0">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Button fx:id="ExplorerBtn" blendMode="OVERLAY" layoutX="110.0" layoutY="243.0" mnemonicParsing="false" onAction="#explorer" prefHeight="34.0" prefWidth="116.0">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Button fx:id="TelegramBtn" blendMode="OVERLAY" layoutX="110.0" layoutY="292.0" mnemonicParsing="false" onAction="#telegram" prefHeight="34.0" prefWidth="116.0">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Button fx:id="SettingBtn" blendMode="OVERLAY" layoutX="110.0" layoutY="340.0" mnemonicParsing="false" onAction="#setting" prefHeight="34.0" prefWidth="116.0">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Button fx:id="LogoutBtn" blendMode="OVERLAY" layoutX="14.0" layoutY="555.0" mnemonicParsing="false" onAction="#logout" prefHeight="34.0" prefWidth="45.0">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Label fx:id="welcomeLabel" layoutX="82.0" layoutY="110.0" prefHeight="21.0" prefWidth="174.0" text="Welcome " textFill="#fffefe">
         <font>
            <Font name="SimSun" size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
