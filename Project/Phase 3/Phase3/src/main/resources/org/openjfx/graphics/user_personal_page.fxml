<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="603.0" prefWidth="335.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.openjfx.graphics.pages.UserPersonalPageFXML">
   <children>
      <ImageView fitHeight="603.0" fitWidth="335.0">
         <image>
            <Image url="@../images/UserPersonalPage.jpg" />
         </image>
      </ImageView>
      <Button fx:id="NewTweetBtn" blendMode="OVERLAY" layoutX="101.0" layoutY="119.0" mnemonicParsing="false" onAction="#newTweet" prefHeight="40.0" prefWidth="133.0">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="UserProfileBtn" blendMode="OVERLAY" layoutX="101.0" layoutY="185.0" mnemonicParsing="false" onAction="#userProfile" prefHeight="40.0" prefWidth="133.0">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="ListsBtn" blendMode="OVERLAY" layoutX="101.0" layoutY="262.0" mnemonicParsing="false" onAction="#showLists" prefHeight="40.0" prefWidth="133.0">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="NotificationsBtn" blendMode="OVERLAY" layoutX="101.0" layoutY="341.0" mnemonicParsing="false" onAction="#notifications" prefHeight="40.0" prefWidth="133.0">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="EditProfileBtn" blendMode="OVERLAY" layoutX="101.0" layoutY="415.0" mnemonicParsing="false" onAction="#editProfile" prefHeight="40.0" prefWidth="133.0">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="BackBtn" blendMode="OVERLAY" layoutX="14.0" layoutY="540.0" mnemonicParsing="false" onAction="#back" prefHeight="43.0" prefWidth="66.0">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
