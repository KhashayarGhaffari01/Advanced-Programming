<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="603.0" prefWidth="335.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.openjfx.graphics.pages.NewTweetPageFXML">
   <children>
      <ImageView fitHeight="603.0" fitWidth="335.0">
         <image>
            <Image url="@../images/NewTweet.jpg" />
         </image>
      </ImageView>
      <Button fx:id="NewTweetBtn" blendMode="OVERLAY" layoutX="187.0" layoutY="497.0" mnemonicParsing="false" onAction="#tweet" prefHeight="41.0" prefWidth="63.0">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="Back" blendMode="OVERLAY" layoutX="68.0" layoutY="497.0" mnemonicParsing="false" onAction="#back" prefHeight="41.0" prefWidth="63.0">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Label fx:id="nameLabel" alignment="CENTER" layoutX="193.0" layoutY="139.0" prefHeight="2.0" prefWidth="69.0" text="Khashayar" textFill="WHITE">
         <font>
            <Font name="Trebuchet MS" size="13.0" />
         </font>
      </Label>
      <TextArea fx:id="tweetContent" layoutX="68.0" layoutY="213.0" prefHeight="256.0" prefWidth="199.0" promptText="Your Khweet Contest" wrapText="true" />
   </children>
</AnchorPane>
